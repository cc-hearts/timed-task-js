{
  "name": "timed-task-js",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "engine": "pnpm --filter @repo/engine start",
    "service": "pnpm --filter @repo/service start:dev",
    "puppeteer": "pnpm --filter @repo/puppeteer start",
    "engine:prod": "pnpm --filter @repo/engine prod",
    "build:core": "pnpm --filter=\"./packages/**\" -r --stream build",
    "build:prod": "pnpm --filter @repo/engine build && pnpm --filter @repo/puppeteer build",
    "start:prod": "pnpm --filter @repo/engine start:prod && pnpm --filter @repo/puppeteer start:prod",
    "lint": "eslint --ext .ts . --fix"
  },
  "keywords": [
    "timed task"
  ],
  "author": "heart",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-env": "^7.20.2",
    "@cc-heart/eslint-config": "^0.0.2",
    "@cc-heart/utils": "^1.2.9",
    "@prisma/client": "^4.8.1",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "rollup": "^3.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
